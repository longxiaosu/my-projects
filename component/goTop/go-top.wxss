@import "../../style/iconfont.wxss";

.go-top {
  position: fixed;
  bottom: 80rpx;
  right: 32rpx;
  z-index: 9996;
  background-color: #fff;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  box-shadow: 0px 5px 18px rgba(226, 226, 226, 0.75);
  display: flex;
  flex-direction: column;
  opacity: 0.7;
}

.go-top text {
  font-size: 26rpx;
  text-align: center;
}

.icon-dingbu::before {
  content: '\e70b'
}

.icon-dingbu {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  margin-top: 10rpx;
}