@import "../../style/iconfont.wxss";

.icon-fanhui:before {
  content: '\e65c'; 
}

.icon-shouye:before {
  content: '\e663';
}

/* 顶部要固定定位   标题要居中   自定义按钮和标题要和右边微信原生的胶囊上下对齐 */

.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  color: #000;
  z-index: 9990;
  transition: all 0.4s ease;
  overflow: hidden;
}

.whit-bg {
  background: #fff;
}

/* 标题要居中 */

.nav-title {
  position: absolute;
  text-align: center;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 32rpx;
  letter-spacing: 1px;
  color: #2c2b2b;
  font-weight: 600;
}

.nav-capsule {
  position: absolute;
  left: 0;
  display: flex;
  align-items: center;
  margin-left: 18rpx;
  width: 150rpx;
  justify-content: space-between;
  height: 100%;
  box-sizing: border-box;
}

.menu-box {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  /* background-color: rgba(255, 255, 255, 0.6); */
  /* padding: 2rpx 20rpx; */
  /* border-radius: 25px; */
  /* border: 1px solid #e5e5e5; */
}

.navbar-v-line {
  width: 1px;
  height: 32rpx;
  background-color: #e5e5e5;
  margin: 0 10rpx;
}

.icon-fanhui {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  color: #070200;
  width: 32rpx;
  height: 32rpx;
  padding: 10rpx;
}

.icon-shouye {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  color: #070200;
  width: 32rpx;
  height: 32rpx;
  padding: 10rpx;
}

